@model Competition.App.Common.ViewModels.Competition.CompetitionStandingsViewModel

@{
    ViewBag.Title = "Standings";
}

<h2>@Model.CompetitionName Standings</h2>


<table class="table">
    <tr>
        <th>
            Team
        </th>
        <th>
            Points
        </th>
    </tr>

@foreach (var item in Model.Items) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TeamName)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.Points)
    </td>
</tr>
}

</table>
